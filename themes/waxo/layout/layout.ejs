<%
	var title = '';

	if ( is_home() )
		title = config.title;
	else if (is_archive()){
		title += 'All archives';

		if (is_month()) {
			title += ' : ' + page.year + '/' + page.month;
		}
		else if (is_year()) {
			title += ' : ' + page.year;
		}

		title += ' - ' + config.title;
	}
	else if (is_tag()) {
		if (page.tag) {
			title += 'Tag : ' + page.tag;
		}
		else {
			title += 'All tags';
		}

		title += ' - ' + config.title;
	}
	else if (is_category()) {
		if (page.category) {
			title += 'Category : ' + page.category;
		}
		else {
			title += 'All categories';
		}

		title += ' - ' + config.title;
	}
	else {
	  title += page.title + ' - ' + config.title;
	}

	var type = 'article';
	if( is_home() )
		type = 'site';

	var currentUrl = config.url + page.path;
	if( is_home() )
		currentUrl = config.url;

	var description = 'Blog en vadrouille';
	if( page.excerpt )
		description = page.excerpt;

%><!DOCTYPE html>
